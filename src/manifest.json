{
  "manifest_version": 3,
  "name": "Nohio",
  "version": "0.1.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["*://*.nationstates.net/*"],
      "js": ["content.ts"]
    }
  ],
  "action": {}
}
